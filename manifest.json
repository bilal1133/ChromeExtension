{
    "name": "GetFiverrTitle",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": [
        "activeTab",
        "https://www.fiverr.com/*"
    ],
        "background": {
        "scripts": [
            "background.js"
        ]
    },
 
    "browser_action": {
       
        "default_icon": {
            "16": "img/16.png",
            "32": "img/32.png",
            "48": "img/48.png",
            "128": "img/128.png"
        }
    },
    "icons": {
        "16": "img/16.png",
        "32": "img/32.png",
        "48": "img/48.png",
        "128": "img/128.png"
    },
    "manifest_version": 2
}